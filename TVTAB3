<script>
function getData() {fetch("https://tv4wap.neocities.org/IPTV/ENT.txt")
 .then(res => {return res.json(); }) 
 .then(json => {console.log(json.data);const html  = json.list
  .map(function(item) {
 return  " <div align='center' class='rsz' data-group='ENTERTAINMENT'  data-name='" + item.name + "'><a  target='frame' href='https://tvlive.wapka.xyz/p16?"+item.id+"'><p>"+item.name+"</p></a></div></div>"; })
 .join("");
 console.log(html);
document.querySelector("#app0" ).insertAdjacentHTML("afterbegin", html); })
 .catch(error => { console.log(error); });}
getData();
</script>



